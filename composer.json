{
    "name": "kevinrider/laravel-etrade",
    "description": "PHP E*Trade REST API v1 Client Library for Laravel",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kevin Rider",
            "email": "github@thebackwaters.com"
        }
    ],
    "require": {
        "php": "^8.3|^8.4|^8.5",
        "guzzlehttp/guzzle": "^7.10",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "guzzlehttp/oauth-subscriber": "^0.8"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "mockery/mockery": "^1.6.2",
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.1",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "KevinRider\\LaravelEtrade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KevinRider\\LaravelEtrade\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "coverage": "vendor/bin/pest --coverage",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "KevinRider\\LaravelEtrade\\LaravelEtradeServiceProvider"
            ]
        }
    }
}