{
    "name": "kevinrider/laravel-etrade",
    "description": "PHP E*Trade REST API v1 Client Library for Laravel",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kevin Rider",
            "email": "github@thebackwaters.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "guzzlehttp/guzzle": "^7.10",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.1",
        "laravel/pint": "^1.24",
        "mockery/mockery": "^1.6.2",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "KevinRider\\LaravelEtrade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "coverage": "vendor/bin/phpunit --coverage-text"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "KevinRider\\LaravelEtrade\\LaravelEtradeServiceProvider"
            ]
        }
    }
}
